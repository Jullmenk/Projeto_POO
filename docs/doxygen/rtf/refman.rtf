00365     \par
00366     {\cf17 auto}& empresLista = bib.getEmprestimosPorCategoria();\par
00367     {\cf17 auto} it = empresLista.find(livro->getCategoria());\par
00368     {\cf19 if} (it == empresLista.end()) \{\par
00369         cout << {\cf22 "Nenhum livro dessa categoria foi requisitado.\\n"};\par
00370         {\cf19 return};\par
00371     \}\par
00372 \par
00373     {\cf20 // Itera\'E7\'E3o de tr\'E1s para frente com iteradores reversos}\par
00374     {\cf19 for} ({\cf17 auto} rit = it->second.rbegin(); rit != it->second.rend(); ++rit) \{\par
00375         {\cf17 auto}& emprestimo = *rit;\par
00376         {\cf19 if} ((emprestimo.getNifEmprestimo() == pessoa->getNIF()) && (emprestimo.getIDLivroEmprestimo() == livro->getCodigo())) \{\par
00377             livro->setDisponivel();\par
00378             pessoa->decrementarEmprestimosAtivos();\par
00379             time_t dataAtual = time({\cf17 nullptr});\par
00380             {\cf19 if}(emprestimo.getDataDevolucaoEmprestimo() < dataAtual)\{\par
00381                 pessoa->incrementarMulta();\par
00382                 pessoa->EnviarNotificacoesdeAtraso();\par
00383                 system({\cf22 "pause"});\par
00384             \}\par
00385             cout << {\cf22 "Livro devolvido com sucesso.\\n"};\par
00386             bib.transformarReservaEmEmprestimo(emprestimo.getCategoriaLivroEmprestimo(),emprestimo.getIDLivroEmprestimo());\par
00387             system({\cf22 "pause"});\par
00388             {\cf19 return}; \par
00389         \}\par
