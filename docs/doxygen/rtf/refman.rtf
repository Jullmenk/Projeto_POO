00339     {\cf18 string} id_Pessoa,id_Livro;\par
00340     {\cf18 bool} findUser={\cf17 false},findLivro={\cf17 false};\par
00341     Pessoa *pessoa={\cf17 nullptr};\par
00342     Geral *livro={\cf17 nullptr};\par
00343     retorno user = RetornarType_String_User();\par
00344     {\cf19 while} (!findUser)\par
00345     \{\par
00346         {\cf19 if}(!bib.listarLeitores({\cf17 true},user.categoria)){\cf19 return};\par
00347         cout << {\cf22 "\\t Digite o NIF do utilizador : "};\par
00348         cin>> id_Pessoa;\par
00349         pessoa = bib.ProcurarUtilizador(id_Pessoa);\par
00350         {\cf19 if}(pessoa)findUser={\cf17 true};\par
00351     \}\par
00352 \par
00353     pessoa->listarEmprestimos();\par
00354 \par
00355     {\cf19 if}(pessoa->getNumeroDeEmprestimosTotais()<=0){\cf19 return};\par
00356 \par
00357     {\cf19 while} (!findLivro)\par
00358     \{\par
00359         cout<< {\cf22 "\\tDigite o ISBN/ISSN do Livro Deseja Devolver : "};\par
00360         cin >> id_Livro;\par
00361         livro = bib.ProcurarLivro(id_Livro);\par
00362         {\cf19 if}(livro)findLivro={\cf17 true};\par
00363 \par
00364     \}\par
00365     \par
00366     {\cf17 auto}& empresLista = bib.getEmprestimosPorCategoria();\par
